#include "Variables.inc"

DrawGame()
{

    currentTime = getTime();
    if (currentTime - CoinsAnimationTimer.time > CoinsAnimationTimer.delay)
    {
        CoinsAnimationTimer.time = currentTime;
        CoinColor = (CoinColor == 0) ? 1 : 0;
    }
    
    if (currentTime - MapBackGroundAnimationTimer.time > MapBackGroundAnimationTimer.delay)
    {
        MapBackGroundAnimationTimer.time = currentTime;

        if(AlphaMapIterSide == 0)
        {
            AlphaMapIter++;
            if(AlphaMapIter == 2)
            {
                AlphaMapIterSide = 1;
            }
        }
        else if(AlphaMapIterSide == 1)
        {
            AlphaMapIter--;
            if(AlphaMapIter == 0)
            {
                AlphaMapIterSide = 0;
            }
        }
        //printf("AlphaMapIter: %d\n", AlphaMapIter);
        
    }


    for(new screenNumber = 0; screenNumber<3; screenNumber++)
    {
        GFX_setRenderTarget(screenNumber);
        
        GFX_drawBakedImage([120, 120], BackGroundAlpha[AlphaMapIter], 0x0, 0, MIRROR_BLANK, screenNumber);

        DrawCoins(screenNumber);

        if(SELF_ID == HeroFacelet.module && screenNumber == HeroFacelet.screen)
        {
            DrawHero();
        }


        GFX_drawText([200, 230], 4, 0, 0, TEXT_ALIGN_CENTER, 0xFFFFFFFF, GAME_VERSION);
        GFX_render();

    }
}

DrawCoins(screenNumber)
{

    for(new i = 0; i < 12; i++)
    {
        for(new j = 0; j < 12; j++)
        {
            if (MAP(screenNumber)(i)(j) == 4)
            {
                new rect[GFX_RECTANGLE];
                rect.h = 4;
                rect.w = 4;

                rect.x = j * 240/12 + 8;
                rect.y = i * 240/12 + 8;

                GFX_drawRectangle(rect, CoinsColors[CoinColor]);
            }
        }
    }

}

DrawFinishPoint()
{
    new rectF[5][GFX_RECTANGLE];

    new step = 3;

    for(new i = 0; i < 5; i++)
    {
        rectF[0].h = 0;
        rectF[0].w = 0;
    }

    if (MAP(screenNumber)(i)(j) == 2)
    {
        rect.x = j * 240/12;
        rect.y = i * 240/12;

        GFX_drawRectangle(rect, GFX_fromARGB8888(255, 000, 255, 000));
    }
}


chekWictoryOrGameOver()
{
    //printf("HeroScreen: %d, HeroGridPos.yPos: %d, HeroGridPos.xPos: %d\n", HeroFacelet.screen, HeroGridPos.yPos, HeroGridPos.xPos);
    switch(MAP(HeroFacelet.screen)(HeroGridPos.yPos)(HeroGridPos.xPos))
    {
        case 2:
        {
            new data[MESSAGE_SIZE] = [0, ...];

            data[0] = (e_MESSAGE_WICTORY << 0) | (0 << 8) | (0 << 16) | (0 << 24);
            broadcastMessage(data);
            //currentLevel +=1;
            GameStatus = e_GAME_STATUS_WIN;

        }
        case 3:
        {
            
        }
    }

}

SendStartEvent()
{
    new data[MESSAGE_SIZE] = [0, ...];

    data[0] = (e_MESSAGE_EVENT_START << 0) | (0 << 8) | (0 << 16) | (SELF_ID << 24);
    data[1] = (0 << 0) | (0 << 8) | (0 << 16) | (0 << 24);

    broadcastMessage(data);
}

SendCompleteLevelInit()
{
    new data[MESSAGE_SIZE] = [0, ...];

    if(SELF_ID == 0)
    {
        LEVEL_MODULE_INIT[SELF_ID] = 1;
    }
    else
    {
        new MODULE_DEST = 0;

        sendPacketIndex[MODULE_DEST] += 1;

        data[0] = (e_MESSAGE_LEVEL_INIT_COMPLETE << 0) | (MODULE_DEST << 8) | (0 << 16) | (SELF_ID << 24);
        data[1] = (sendPacketIndex[MODULE_DEST] << 0) | (0 << 8) | (0 << 16) | (0 << 24);

        broadcastMessage(data);
    }
}

SendLevelInit()
{
    new data[MESSAGE_SIZE] = [0, ...];
  
    LevelInitPacket += 1;

    data[0] = (e_MESSAGE_LEVEL_INIT << 0) | (0 << 8) | (0 << 16) | (SELF_ID << 24);
    data[1] = (LevelInitPacket << 0) | (0 << 8) | (0 << 16) | (0 << 24);

    broadcastMessage(data);
}

SendGameOver()
{
    new data[MESSAGE_SIZE] = [0, ...];
    data[0] = (e_MESSAGE_GAMEOVER << 0) | (0 << 8) | (0 << 16) | (0 << 24);
    broadcastMessage(data);
    //currentLevel +=1;
    GameStatus = e_GAME_STATUS_GAMEOVER;
}


BakeStaticLevelMap()
{
    for(new screenNumber = 0; screenNumber<3; screenNumber++)
    {
        GFX_bakeImage(screenNumber, SIZE_SCREEN, SIZE_SCREEN, FORMAT_RGB565);
        GFX_clear(0xFF000000);
        new centerX = 0;
        new centerY = 0;
        new rect[GFX_RECTANGLE];
        rect.h = 20;
        rect.w = 20;

        for(new i = 0; i < 12; i++)
        {
            for(new j = 0; j < 12; j++)
            {
                if (MAP(screenNumber)(i)(j) == 1)
                {
                    rect.x = j * 240/12;
                    rect.y = i * 240/12;
                    centerX = rect.x + 10;
                    centerY = rect.y + 10;
                    //GFX_drawRectangle(rect, GFX_fromARGB8888(255, 153, 000, 204));
                    DrawCellWall(screenNumber, i, j);
                    //GFX_drawImageXY(centerX, centerY, 0xFF, 0, 0, MIRROR_BLANK, ImageWall);
                }

                if (MAP(screenNumber)(i)(j) == 2)
                {
                    rect.x = j * 240/12;
                    rect.y = i * 240/12;

                    GFX_drawRectangle(rect, GFX_fromARGB8888(255, 000, 255, 000));
                }

                if (MAP(screenNumber)(i)(j) == 3)
                {

                    rect.x = j * 240/12;
                    rect.y = i * 240/12;

                    centerX = rect.x + 10;
                    centerY = rect.y + 10;

                    //GFX_drawRectangle(rect, GFX_fromARGB8888(255, 137, 239, 253));

                    GFX_drawImageXY(centerX, centerY, 0xFF, 0, 0, MIRROR_BLANK, ImageSpike);
                }
            }
        }

        GFX_render();

    }
}


DrawCellWall(screenNumber, i, j)
{

    GFX_drawImageXY(j*20+10, i*20+10, 0xFF, 0, 0, MIRROR_BLANK, ImageWallBack);

    if(j == 0)
    {
        GFX_drawImageXY(j*20+10, i*20+10, 0xFF, 0, 270, MIRROR_BLANK, ImageWallStick);
    }
    if(j == 11)
    {
        GFX_drawImageXY(j*20+10, i*20+10, 0xFF, 0, 90, MIRROR_BLANK, ImageWallStick);
    }
    if(i == 0)
    {
        GFX_drawImageXY(j*20+10, i*20+10, 0xFF, 0, 0, MIRROR_BLANK, ImageWallStick);
    }
    if(i == 11)
    {
        GFX_drawImageXY(j*20+10, i*20+10, 0xFF, 0, 180, MIRROR_BLANK, ImageWallStick);
    }


    if(i + 1 < SIZE_GRID_MAP)
    {
        if(WayIsFree(MAP(screenNumber)(i+1)(j)))
        {
            GFX_drawImageXY(j*20+10, i*20+10, 0xFF, 0, 180, MIRROR_BLANK, ImageWallStick);
        }
    }
    if(i - 1 >= 0)
    {
        if(WayIsFree(MAP(screenNumber)(i-1)(j)))
        {
            GFX_drawImageXY(j*20+10, i*20+10, 0xFF, 0, 0, MIRROR_BLANK, ImageWallStick);
        }
    }

    if(j + 1 < SIZE_GRID_MAP)
    {
        if(WayIsFree(MAP(screenNumber)(i)(j+1)))
        {
            GFX_drawImageXY(j*20+10, i*20+10, 0xFF, 0, 90, MIRROR_BLANK, ImageWallStick);
        }
    }
    if(j - 1 >= 0)
    {
        if(WayIsFree(MAP(screenNumber)(i)(j-1)))
        {
            GFX_drawImageXY(j*20+10, i*20+10, 0xFF, 0, 270, MIRROR_BLANK, ImageWallStick);
        }
    }

}



